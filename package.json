{
  "name": "@whisky-gaming/ui",
  "private": false,
  "version": "1.0.7",
  "description": "React UI Components for Whisky Gaming Protocol",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whisky-gaming/whisky-ui.git"
  },
  "homepage": "https://whisky.gg",
  "bugs": {
    "url": "https://github.com/whisky-gaming/whisky-ui/issues"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --external react --dts",
    "dev": "tsup src/index.ts --watch --format cjs,esm --external react --dts",
    "lint": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.27.0",
    "@preact/signals-react": "^1.3.8",
    "@solana/spl-token": "^0.3.8",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/web3.js": "^1.93.0",
    "@whisky-gaming/core": "^1.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-components": "^6.0.8",
    "tone": "^14.7.77",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.0.11",
    "assert": "^2.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@coral-xyz/anchor": "^0.27.0",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/web3.js": "^1.93.0",
    "@whisky-gaming/core": "^1.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "peerDependenciesMeta": {
    "@solana/wallet-adapter-react": {
      "optional": true
    }
  },
  "keywords": [
    "solana",
    "gaming",
    "blockchain",
    "casino",
    "defi",
    "whisky",
    "protocol",
    "ui",
    "react",
    "web3",
    "components"
  ],
  "author": "Sarthak Kapila",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
